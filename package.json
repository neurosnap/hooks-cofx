    "@types/node": "^10.12.23",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "cofx": "^2.1.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.3",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-testing-library": "^6.0.2",
    "ts-jest": "^23.10.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.18.0",
