{
  "name": "use-cofx",
  "version": "1.0.1",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "npmClient": "yarn",
  "license": "MIT",
  "author": "Eric Bower",
  "description": "Use cofx with react hooks",
  "repository": {
    "type": "git",
    "url": "git://github.com/neurosnap/use-cofx.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc --noEmit && jest index.test.ts",
